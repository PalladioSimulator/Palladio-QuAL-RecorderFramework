<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.palladiosimulator.recorderframework.feature"
      label="RecorderFramework Feature"
      version="5.2.0"
      plugin="org.palladiosimulator.branding"
      license-feature="org.palladiosimulator.license"
      license-feature-version="1.0.0">

   <description url="http://palladiosimulator.org">
      Allows the specification of recorders that can store measured metrics. Recorders can store such measurements in files, main memory, data bases and the like.
   </description>

   <requires>
      <import plugin="org.jscience" version="4.1.3" match="greaterOrEqual"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.debug.ui" version="3.5.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.swt" version="3.5.1" match="greaterOrEqual"/>
      <import plugin="org.apache.log4j" version="1.2.13" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.probeframework" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.measurementframework" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.metricspec" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2"/>
      <import plugin="org.palladiosimulator.commons"/>
      <import plugin="org.eclipse.emf.ecore" version="2.4.2" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface"/>
      <import plugin="org.palladiosimulator.edp2.ui" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.dao" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.edp2.util" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.sensorframework" version="1.1.0" match="greaterOrEqual"/>
      <import plugin="de.uka.ipd.sdq.sensorframework.dialogs" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.recorderframework" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="org.palladiosimulator.branding"/>
   </requires>

   <plugin
         id="org.palladiosimulator.recorderframework"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.recorderframework.edp2"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.palladiosimulator.recorderframework.sensorframework"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
